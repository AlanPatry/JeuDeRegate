<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="302.0" prefWidth="512.0" styleClass="dialog" stylesheets="@LoginDialog.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ensicaen.regatta.mvp.view.LoginView">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="237.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="312.0" minWidth="10.0" prefWidth="240.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="41.0" minWidth="0.0" prefWidth="0.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="50.0" vgrow="NEVER" />
        <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="NEVER" />
        <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints maxHeight="100.0" minHeight="50.0" prefHeight="100.0" vgrow="NEVER" />
      <RowConstraints maxHeight="50.0" minHeight="0.0" prefHeight="0.0" vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Label alignment="CENTER" prefHeight="16.0" prefWidth="160.0" styleClass="text" text="%login.message.text1" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Label alignment="CENTER" prefHeight="16.0" prefWidth="160.0" styleClass="text" text="%login.message.text2" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label alignment="CENTER" prefHeight="16.0" prefWidth="160.0" styleClass="text" text="%login.message.text3" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label alignment="CENTER" prefHeight="16.0" prefWidth="160.0" styleClass="text" text="%login.message.text4" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <TextField fx:id="_nickName" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Button alignment="TOP_CENTER" contentDisplay="CENTER" defaultButton="true" onAction="#onClickOnStartGame" prefHeight="24.0" prefWidth="154.0" text="%launch.button.text" translateX="-121.0" translateY="15.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
        <Label fx:id="_errorMessage" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="16.0" prefWidth="292.0" textFill="#ef0000" translateY="-20.0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" />
        <ComboBox fx:id="_comboBox1" prefHeight="24.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <ComboBox fx:id="_comboBox2" prefHeight="24.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <ComboBox fx:id="_comboBox3" prefHeight="24.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Button mnemonicParsing="false" onAction="#switchLanguage" prefHeight="24.0" prefWidth="36.0" text="EN" translateX="160.0" translateY="20.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button mnemonicParsing="false" onAction="#switchLanguage" prefHeight="24.0" prefWidth="36.0" text="FR" translateX="200.0" translateY="20.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
